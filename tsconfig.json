{
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "strict": true,
    "strictPropertyInitialization": false,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "forceConsistentCasingInFileNames": true,
    "rootDir": "./",
    "sourceRoot": "./",
    "paths": {
      "@notadd/*": ["@notadd/*"]
    }
  }
}